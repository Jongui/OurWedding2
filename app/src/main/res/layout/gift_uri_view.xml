<?xml version="1.0" encoding="utf-8"?>
<joaogd53.com.br.customViews.InternetAddressView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:paddingBottom="6dp"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    android:paddingTop="9dp"
    android:orientation="horizontal">

    <ImageView
        android:layout_width="40dp"
        android:layout_height="match_parent"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/ic_web" />

    <TextView
        android:id="@+id/content_name_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:text="@string/gotToWebSite"
        android:textColor="@color/mainTextColor"
        android:textSize="18dp"
        android:textStyle="bold" />

</joaogd53.com.br.customViews.InternetAddressView>